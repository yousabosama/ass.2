#include <stdio.h>
int countHoles(int num)
{
    int holes=0,d;
    while(num!=0)
{
        d=num%10;
        if(d==0||d==4||d==6||d==9) holes+=1;
        else if(d==8) holes+=2;
        num/=10;
    }
    return holes;
}
int main()
{
    int n;
    scanf("%d",&n);
    printf("%d", countHoles(n));
    return 0;
}
